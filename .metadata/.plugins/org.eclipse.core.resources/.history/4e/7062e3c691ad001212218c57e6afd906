package phone;

import java.awt.GridLayout;

import javax.swing.JPanel;
import javax.swing.JScrollPane;

public class Menu extends JPanel{

	public Menu() {
		
		String[] menuItems = {
			"Item 1", 
			"Item 2", 
			"Item 3", 
			"Item 4", 
			"Item 5"
				
		}; 
		
		JPanel panel = new JPanel(new GridLayout(menuItems.length, 1)); 
		
		this.add(panel); 
		
	}
	
}
